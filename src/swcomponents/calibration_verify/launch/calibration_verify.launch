	<param name="image_rgb" value="/kinect_right/rgb/image_rect_color"/>
	<param name="image_depth" value="/kinect_right/depth_registered/image_raw"/>
	<param name="camera_info" value="/kinect_right/rgb/camera_info"/>
</node>
-->

</launch>
