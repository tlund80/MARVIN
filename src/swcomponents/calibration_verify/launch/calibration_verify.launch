<launch>
<env name="ROS_MASTER_URI" value="http://tek-marvin-3.sandbox.tek.sdu.dk:11311"/>
<include file="$(find marvin)/launch/marvin.machine" />

<param name="/workcell" value="$(find marvin)/scene/Marvin_calibration_verify_ur1.model.wc.xml"/>
<node name="UR1" pkg="universalrobots" type="ur" respawn="false" machine="marvin-4">
<param name="properties" value="$(find universalrobots)/ur1.prop.xml"/>
</node> 

<!-- run chessboard detector node -->
<node name="stereo_chessboard_detector" pkg="calibration_verify" type="stereo_chessboard_detector" output="screen">
        <param name="image_left" value="/bumblebeeLeft/left/image_rect_color"/>
        <param name="image_right" value="/bumblebeeLeft/right/image_rect_color"/>
        <param name="calib_rect" value="/bumblebeeLeft/stereo_calib_rect"/>
</node>

<!--
