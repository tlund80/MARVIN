 <RigidDevice device="PG70">
 	<ForceLimit joint="RightFinger">51.0</ForceLimit>	
	<ForceLimit joint="LeftFinger">51.0</ForceLimit>	
	<KinematicBase frame="Base">
		<MaterialID>Plastic</MaterialID>
	</KinematicBase> 
    
	<RigidJoint joint="RightFinger">
	  <Mass>0.2</Mass>
	  <EstimateInertia />
	  <MaterialID>Rubber</MaterialID>
	</RigidJoint>

	<RigidJoint joint="LeftFinger">
	  <Mass>0.2</Mass>
	  <EstimateInertia />
	  <MaterialID>Rubber</MaterialID>
	</RigidBody>
  </RigidDevice>


