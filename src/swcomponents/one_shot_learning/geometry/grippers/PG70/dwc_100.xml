<DynamicWorkcell workcell="rwc_100.xml">
 <PhysicsEngine>
   <!--CollisionMargin>0.001</CollisionMargin-->
   <RestitutionModel>Newton</RestitutionModel>
   <ContactModel>Guendel</ContactModel>
   <AutoDisable>true</AutoDisable>
   <Property name="StepMethod">WorldStep</Property> 
 </PhysicsEngine>
 
 <Gravity>0 0 0</Gravity>
 
 <!-- define different materials, and for each material define properties for the material -->
 <MaterialData>
 	<Default>Plastic</Default>
    <Material id="Plastic"/>
	<Material id="Rubber">
	 	<Description>bla bla bla</Description>
    </Material>
 </MaterialData>
 
 <!-- define the material data map, which is a map that defines properties between ma -->
 <FrictionMap>
  <Pair first="Plastic" second="Plastic"> 
	<FrictionData type="Coulomb"> <Mu>0.4</Mu> </FrictionData> </Pair>
  <Pair first="Rubber" second="Rubber"> 
	<FrictionData type="Coulomb"> <Mu>1.2</Mu> </FrictionData> </Pair>
  <Pair first="Plastic" second="Rubber"> 
	<FrictionData type="Coulomb"> <Mu>1.0</Mu> </FrictionData> </Pair>
 </FrictionMap>
 
 <!-- specify the object types that are available -->
 <ObjectTypeData>
  <Default>hardObj</Default>
  <ObjectType id="hardObj">
   <Description>A hard object, with low elasticity</Description> </ObjectType>
  <ObjectType id="softObj">
 	<Description>A soft object, with high elasticity</Description> </ObjectType>	
 </ObjectTypeData>
 
  <!-- We all so need a contact data map. This map will contain contact 
 	    data information such as restitution coefficients between possible contacts -->
 <ContactMap>
  <Pair first="hardObj" second="hardObj"> 
	<ContactData type="Newton"><cr>0.0</cr></ContactData></Pair>
  <Pair first="hardObj" second="softObj"> 
  	<ContactData type="Newton"><cr>0.0</cr></ContactData></Pair>
  <Pair first="softObj" second="softObj"> 
  	<ContactData type="Newton"><cr>0.0</cr></ContactData></Pair>
 </ContactMap>
  
 <!-- You can override the default contactmodel for a specific set of bodies -->
 <ContactModel first="fnameA" second="fnameB" type="Mirtich" />
 
 <!-- Now define all the rigid bodies that exist in the workcell, remember to
      reference the RigidBody to a movableframe -->
 <RigidBody frame="Object">
  <Mass>0.2</Mass>
  <EstimateInertia />
  <Integrator>Euler</Integrator>
  <MaterialID>Plastic</MaterialID>
 </RigidBody>

   <KinematicDevice device="PG70">
 	<ForceLimit joint="RightFinger">2.0</ForceLimit>	
	<ForceLimit joint="LeftFinger">2.0</ForceLimit>	
	
 	<!--ForceLimit joint="f1.Joint2">1.0</ForceLimit>	
	<ForceLimit joint="f1.Joint3">0.7</ForceLimit>	
	<ForceLimit joint="F2Joint1">1.0</ForceLimit>
	<ForceLimit joint="f2.Joint2">1.0</ForceLimit>	
	<ForceLimit joint="f2.Joint3">0.7</ForceLimit>	
	<ForceLimit joint="F3Joint1">1.0</ForceLimit>	
	<ForceLimit joint="f3.Joint2">1.0</ForceLimit>	
	<ForceLimit joint="f3.Joint3">0.7</ForceLimit-->	
	
    <!-- Specify the type of base. a fixed base is the most common, 
		 this will be fixed in the initial state -->
	<!-- if a kinematic base, use this -->
	<KinematicBase frame="Base">
		<MaterialID>Plastic</MaterialID>
	</KinematicBase> 
	<!-- if a rigidbase is created then make sure it references a movable frame-->
	<RigidJoint joint="RightFinger">
	  <Mass>0.1</Mass>
	  <EstimateInertia />
	  <MaterialID>Rubber</MaterialID>
	</RigidJoint>

	<RigidJoint joint="LeftFinger">
	  <Mass>0.1</Mass>
	  <EstimateInertia />
	  <MaterialID>Rubber</MaterialID>
	</RigidBody>
  </KinematicDevice>
</DynamicWorkcell>
